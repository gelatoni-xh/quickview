{
  "manifest_version": 3,
  "name": "QuickView",
  "description": "Personal dashboard new tab",
  "version": "0.1.0",

  "chrome_url_overrides": {
    "newtab": "index.html"
  },

  "host_permissions": [
    "https://worldtimeapi.org/*"
  ],

  "background": {
    "service_worker": "background.js"
  },

  "permissions": [
    "storage"
  ]
}